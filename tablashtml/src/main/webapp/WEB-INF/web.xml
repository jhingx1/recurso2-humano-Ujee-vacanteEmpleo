<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
        <servlet-name>CatalogoProductos</servlet-name>
        <servlet-class>com.servlets.CatalogoProductos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TablaHTML</servlet-name>
        <servlet-class>com.servlets.TablaHTML</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CatalogoProductos</servlet-name>
        <url-pattern>/catalogo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TablaHTML</servlet-name>
        <url-pattern>/tabla</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
  
</web-app>
